class A
{
	 void show()
	 {
		 System.out.println("i am in class A");
	 }
}
class B extends A
{
	void show()
	{
		System.out.println("i am in class B");
	}
	void m1()
	{
		System.out.println("1");
		super.show();
		System.out.println("2");

		
	}
}
public class SuperTest {
	public static void main(String[] args) {
		B ob=new B();
		ob.m1();
	}

}
